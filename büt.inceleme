        "    from IPython.display import HTML\n",
        "\n",
        "    fig, ax = plt.subplots()\n",
        "\n",
        "    line, = ax.plot([], \"*\")\n",
        "\n",
        "    ax.set_xlim(-1.1, 1.1)\n",
        "    ax.set_ylim(0.0, max(position)+5)\n",
        "\n",
        "    def animate(time_step):\n",
        "        x = np.array([0.0])\n",
        "        y = np.array(position[time_step])\n",
        "        line.set_data((x, y))\n",
        "        return line\n",
        "    \n",
        "    anim_pos = FuncAnimation(fig, animate, \n",
        "                        frames=len(time_list), \n",
        "                        interval=50  # determines how fast your animation will be aired.\n",
        "                        )\n",
        "    # HTML(anim_pos.to_html5_video())\n",
        "    return HTML(anim_pos.to_html5_video())\n"
