    {
      "cell_type": "code",
      "source": [
        "def animate_physical_quantities(time_list, velocity, position):\n",
        "    %config InlineBackend.figure_format = 'retina'\n",
        "    # Enable interactive plot\n",
        "    %matplotlib notebook\n",
        "    # Call notebook interactions again. This is needed due to a bug in Matplotlib.\n",
        "    %matplotlib notebook\n",
        "    # %matplotlib notebook\n",
        "    from IPython.display import HTML\n",
        "\n",
        "    fig = plt.figure(figsize=(12, 6))\n",
        "    ax1 = plt.subplot(1, 2, 1)\n",
        "    ax2 = plt.subplot(1, 2, 2)\n",
        "\n",
        "    ax1.set_xlim(0, 1)\n",
        "    ax1.set_ylim(min(velocity) - 5, max(velocity) + 5)\n",
        "    ax2.set_xlim(-1.1, 1.1)\n",
        "    ax2.set_ylim(0.0, max(position)+5)\n",
        "\n",
        "    ax1.set_title(\"Velocity\", fontsize=24)\n",
        "    ax2.set_title(\"Position\", fontsize=24)\n",
        "\n",
        "    ax1.set_ylabel(\"Speed (m/sec)\", fontsize=16)\n",
        "    ax2.set_ylabel(\"Height (m)\", fontsize=16)\n",
        "\n",
        "    ax1.axes.xaxis.set_visible(False)\n",
        "    ax2.axes.xaxis.set_visible(False)\n",
        "\n",
        "    line1, = ax1.plot([], \"r\")\n",
        "    line2, = ax1.plot([], \"vr\")\n",
        "    line3, = ax2.plot([], \"o\")\n",
        "    line = [line1, line2, line3]\n",
        "\n",
        "\n",
        "    def animate_all(time_step):\n",
        "        x1 = np.array([0.5, 0.5])\n",
        "        y1 = np.array([0.0, velocity[time_step]])\n",
        "        x2 = np.array([0.5])\n",
        "        y2 = np.array([velocity[time_step]])\n",
        "        x3 = np.array([0.0])\n",
        "        y3 = np.array(position[time_step])\n",
        "        line[0].set_data(x1, y1)\n",
        "        line[1].set_data(x2, y2)\n",
        "        line[2].set_data(x3, y3)\n",
        "        return line\n",
        "    \n",
        "    anim_pos = FuncAnimation(fig, animate_all, \n",
        "                        frames=len(time_list), \n",
        "                        interval=50  # determines how fast your animation will be aired.\n",
        "                        )\n",
        "    # HTML(anim_pos.to_html5_video())\n",
        "    return HTML(anim_pos.to_html5_video())"
      ],
