        "def animate_velocity(time_list, velocity):\n",
        "    %config InlineBackend.figure_format = 'retina'\n",
        "    # Enable interactive plot\n",
        "    %matplotlib notebook\n",
        "    # Call notebook interactions again. This is needed due to a bug in Matplotlib.\n",
        "    %matplotlib notebook\n",
        "    from IPython.display import HTML\n",
        "\n",
        "    fig, ax = plt.subplots()\n",
        "\n",
        "    line, = ax.plot([])\n",
        "\n",
        "    ax.set_xlim(0, 1)\n",
        "    ax.set_ylim(min(velocity), max(velocity))\n",
        "\n",
        "    def animate(time_step):\n",
        "        x = np.array([0.5, 0.5])\n",
        "        y = np.array([0.0, velocity[time_step]])\n",
        "        line.set_data((x, y))\n",
        "        return line\n",
        "\n",
        "    anim_vel = FuncAnimation(fig, animate, \n",
        "                        frames=len(time_list), \n",
        "                        interval=50  # determines how fast your animation will be aired.\n",
        "                        )\n",
        "    return HTML(anim_vel.to_html5_video())"
      ],
